{
  "name": "react-boilerplate-webpack",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "start:local": "cross-env NODE_ENV=development ENV=local npm run start",
    "start:dev": "cross-env NODE_ENV=development ENV=dev npm run start",
    "start:prod": "cross-env NODE_ENV=production ENV=prod npm run start",
    "build:test": "cross-env NODE_ENV=development ENV=local CI=false webpack",

    "build": "CI=false webpack --mode production",
    "build:local": "cross-env NODE_ENV=development ENV=local npm run build",
    "build:dev": " cross-env NODE_ENV=development ENV=dev npm run build",
    "build:prod": "cross-env NODE_ENV=production ENV=prod npm run build",
    "api": "json-server -p 3001 -w api/db.json --routes api/routes.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "antd": "^4.1.2",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "lodash.debounce": "^4.0.8",
    "lodash.omit": "^4.5.0",
    "react": "^16.13.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.3.1",
    "react-input-mask": "^2.0.4",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "typesafe-actions": "^5.1.0",
    "@babel/polyfill": "^7.8.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "@types/jest": "^25.2.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-dom": "^16.9.6",
    "@types/react-input-mask": "^2.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "desvg-loader": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "http-proxy-middleware": "^1.0.3",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "svg-loader": "0.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.2",
    "typescript": "~3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
